{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "sitemap": {
      "baseUrl": "https://uraniumui.gh.enisn-projects.io/",
      "priority": 0.1,
      "changefreq": "monthly"
    },
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "en/images/**"
        ]
      }
    ],
    "output": "../_site",
    "template": [
      "default",
      "modern",
      "templates/uranium"
    ],
    "globalMetadata": {
      "_appName": "UraniumUI",
      "_appTitle": "UraniumUI",
      "_appLogoPath": "images/logo.png",
      "_enableSearch": true,
      "pdf": false
    }
  }
}
